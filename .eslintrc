{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/named": 2,
    "import/namespace": 2,
    "class-methods-use-this": "off",
    "no-useless-constructor": "off",
    "no-empty-function": "off",
    "no-unused-vars": "off",
    "below": "off",
    "radix": "off",
    "no-tabs":"off",
    "import/no-extraneous-dependencies": "off",
    "no-shadow":"off"
  }
}
